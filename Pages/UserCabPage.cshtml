@page
@model ah4cClientApp.Pages.UserCabPageModel
@{


    var isdisplay = "none";
    if (ViewData["showerror"] != null && ViewData["showerror"] as string == "true" && ViewData["showsuccess"] == null)
    {
        isdisplay = "block";
    }
    else if (ViewData["showsuccess"] != null && ViewData["showsuccess"] as string == "true")
    {
        isdisplay = "block";
    }
}
<link rel="stylesheet" href="./css/site.css" />


<div class="page">
    <div class="banner">
        <div class="banner-content">
            <div class="banner-title"></div>
            <div class="banner-description">Информация о вашем аккаунте</div>
        </div>

    </div>
    <div class="mainsection">
        <div class="avatar">
            <image class="cabinet-avatar" src="assets/medicinepet-10.png" />
        </div>
        <div class="container2">
            <div class="page-maintitle" name="maintitlename">@UserCabPageModel.user.ClientName </div>
            <div class="selection">
                <div class="label-normal">
                    <div class="label-text">@UserCabPageModel.user.ClientLogin</div>
                </div>
            </div>
            <div class="selection">
                <div class="label-normal">
                    <div class="label-text">@UserCabPageModel.user.ClientPassword</div>
                </div>
                <div class="primary">
                    <form method="post">
                        <button class="title4">👁</button>
                    </form>
                </div>
            </div>
            <div class="selection">
                <div class="primary2">
                    <form>
                        <div class="title4">Сохранить</div>
                    </form>
                </div>
            </div>
        </div>
        <div class="seconday">
